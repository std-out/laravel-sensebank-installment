{
    "name": "std-out/laravel-sensebank-installment",
    "description": "SenseBank Installment package for Laravel ",
    "type": "library",
    "keywords": [
        "std-out",
        "laravel-sensebank-installment"
    ],
    "homepage": "https://github.com/std-out/laravel-sensebank-installment",
    "license": "MIT",
    "authors": [
        {
            "name": "nix",
            "email": "yuriyzee@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^9.28|^10.0|^11.0",
        "yuriizee/sensebank-installment-sdk": "^1.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^7.7|^8.0",
        "pestphp/pest": "^1.22",
        "phpunit/phpunit": "^9.5.24"
    },
    "autoload": {
        "psr-4": {
            "StdOut\\LaravelSensebankInstallment\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StdOut\\LaravelSensebankInstallment\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
